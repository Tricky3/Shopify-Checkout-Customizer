	.chrome input[type="checkbox"]:hover,.safari input[type="checkbox"]:hover{border-color:#ccc}
	.chrome input[type="checkbox"]:focus,.safari input[type="checkbox"]:focus{-webkit-box-shadow:0 0 0 2px #2a9dcc;box-shadow:0 0 0 2px #2a9dcc;border-color:transparent}
	.chrome input[type="checkbox"]:checked:after,.safari input[type="checkbox"]:checked:after{-webkit-transform:scale(1);-ms-transform:scale(1);transform:scale(1);opacity:1;-ms-filter:"progid:DXImageTransform.Microsoft.Alpha(Opacity=100)";filter:alpha(opacity=100);visibility:visible}
	.chrome input[type="checkbox"]:after,.safari input[type="checkbox"]:after{content:"";display:block;position:absolute;left:-1px;top:-1px;width:18px;height:18px;-webkit-transform:scale(0.2);-ms-transform:scale(0.2);transform:scale(0.2);-webkit-transition:all 0.2s cubic-bezier(0.3, 0, 0, 1.2);transition:all 0.2s cubic-bezier(0.3, 0, 0, 1.2);background-image:url(//cdn.shopify.com/s/assets/checkout/icons/ico-check-blue.svg-475cfbaeeead1640eb8ec302b1ec6c88.png);background-image:url(//cdn.shopify.com/s/assets/checkout/icons/ico-check-blue-475cfbaeeead1640eb8ec302b1ec6c88.svg),none;background-position:center center;background-repeat:no-repeat;opacity:0;-ms-filter:"progid:DXImageTransform.Microsoft.Alpha(Opacity=0)";filter:alpha(opacity=0);visibility:hidden}
}

.radio-group input[type="radio"]{position:absolute;top:.9em;left:1em}
.ie .radio-group input[type="radio"]{margin-top:3px}
.firefox .radio-group input[type="radio"]{margin-top:5px}
.radio-group input[type="radio"]+label{padding-left:1.75em}
.safari .radio-group input[type="radio"]+label,
.chrome .radio-group input[type="radio"]+label{padding-left:2.25em}
.radio-group label{width:auto !important}input[type="radio"]{float:left}
.firefox input[type="radio"]{margin-top:3px}
.chrome input[type="radio"],.safari input[type="radio"]{-webkit-backface-visibility:hidden;backface-visibility:hidden;-webkit-appearance:none;display:block;width:20px;height:20px;margin:0;border:1px solid #ccc;border-radius:50%;cursor:pointer;-webkit-box-shadow:0 0 0 0 #2a9dcc inset;box-shadow:0 0 0 0 #2a9dcc inset;-webkit-transition:-webkit-box-shadow 0.3s cubic-bezier(0.3, 0, 0, 1.2),background-color 0.3s cubic-bezier(0.3, 0, 0, 1.2),border-color 0.3s cubic-bezier(0.3, 0, 0, 1.2);transition:box-shadow 0.3s cubic-bezier(0.3, 0, 0, 1.2),background-color 0.3s cubic-bezier(0.3, 0, 0, 1.2),border-color 0.3s cubic-bezier(0.3, 0, 0, 1.2)}
.chrome input[type="radio"]:after,.safari input[type="radio"]:after{content:"";display:block;width:20px;height:20px;margin-left:-1px;margin-top:-1px;background-position:center center;border-radius:50%;opacity:0;visibility:hidden;-webkit-transform:scale(0.2);-ms-transform:scale(0.2);transform:scale(0.2);-webkit-transition:opacity 0.2s cubic-bezier(0.3, 0, 0, 1.2) 0.1s,visibility 0.2s cubic-bezier(0.3, 0, 0, 1.2) 0.1s,-webkit-transform 0.2s cubic-bezier(0.3, 0, 0, 1.2) 0.1s;transition:opacity 0.2s cubic-bezier(0.3, 0, 0, 1.2) 0.1s,visibility 0.2s cubic-bezier(0.3, 0, 0, 1.2) 0.1s,transform 0.2s cubic-bezier(0.3, 0, 0, 1.2) 0.1s;background-image:url(//cdn.shopify.com/s/assets/checkout/icons/ico-check-white.svg-38c64285d92e39f89e95d614844967fa.png);background-image:url(//cdn.shopify.com/s/assets/checkout/icons/ico-check-white-38c64285d92e39f89e95d614844967fa.svg),none;background-position:center center;background-repeat:no-repeat}
.chrome input[type="radio"]:hover,.safari input[type="radio"]:hover{border-color:#b3b3b3}
.chrome input[type="radio"]:focus,.safari input[type="radio"]:focus{outline:none}
.chrome input[type="radio"]:checked,.safari input[type="radio"]:checked{background-color:#2a9dcc;border-color:#2a9dcc;-webkit-box-shadow:0 0 0 10px #2a9dcc inset;box-shadow:0 0 0 10px #2a9dcc inset}
