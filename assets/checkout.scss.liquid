@media screen and (min-width: 1000px){
	.main__header p{max-width:50%;margin:0 auto}
}

/*===================================== Modals ====================================*/
.has-modal{overflow:hidden}
.modal{position:fixed;width:100%;background:#fff;top:0;left:0;bottom:0;right:0;z-index:999;overflow:auto;padding:2em 0 1em;display:none}
.modal .wrap{width:90%;max-width:740px;margin:0 auto;padding:0}
.modal .svg{margin:0 auto 2em}
.modal--centered{height:100%;overflow:hidden;display:-webkit-box;display:-webkit-flex;display:-ms-flexbox;display:flex;-webkit-box-align:center;-webkit-align-items:center;-ms-flex-align:center;align-items:center;-webkit-box-pack:center;-webkit-justify-content:center;-ms-flex-pack:center;justify-content:center;padding:0 !important}
.modal--centered .btn{margin-top:2em}
.modal__close{position:absolute;right:0;top:0;padding:.5em .75em;color:#ccc;color:rgba(0,0,0,0.2);font-size:33px;font-size:2.5rem}
.modal__close:hover{color:#999}.policy-header{margin-bottom:1.5em;border-bottom:1px solid #ededed;padding-bottom:1em}
.policy-header .policy__print-link{display:none}
.policy-header__shop-name{color:#999}
.policy-body p{color:#777}

@media all and (min-width: 600px){
	.modal{padding-top:3em}
	.modal .wrap{width:80%}
	.policy-header{margin-bottom:3em;padding-bottom:2em;position:relative}
	.policy-header .policy__print-link{display:block;position:absolute;top:1em;right:0}
	.policy-body p{font-size:18px;font-size:1.25rem;line-height:$line-height;}
}

@media screen and (min-width: 1000px){
	.modal{padding-top:6em;padding-bottom:4em}
	.modal__close{position:fixed;font-weight:300;font-size:35px;font-size:2.5rem}
	.policy__header{margin-bottom:3em;padding-bottom:2.5em}
	.has-modal .footer{position:fixed;width:100%;bottom:0;left:0;z-index:9999}
}

@media screen and (min-width: 1100px){
	.modal{padding-top:8.75em;padding-bottom:8.75em}
}


/*=========================Alternative Payment Methods ============================*/
.alternative-payment-methods{padding:1.5em 1.5em 2em;margin-bottom:1em;border-bottom:1px solid #ededed;position:relative;text-align:center}
.alternative-payment-methods .or{content:"OR";text-align:center;display:block;min-width:18px;padding:0 .5em;margin:-14px 0 0 -14px;position:absolute;top:100%;left:50%;background-color:#fff;border:1px solid #e5e5e5;border-radius:2em;font-size:11px;color:#999;line-height:$line-height;-webkit-box-shadow:0 0 0 8px #fff;box-shadow:0 0 0 8px #fff}
