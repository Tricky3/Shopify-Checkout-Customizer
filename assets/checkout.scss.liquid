/*================================ Call to Action =================================*/
.call-to-action{zoom:1}
.call-to-action:after,
.call-to-action:before{content:"";display:table}
.call-to-action:after{clear:both}
.no-generatedcontent .call-to-action{overflow:hidden}
.step--completed .call-to-action{display:none}
table+.call-to-action,.section__blank-slate+.call-to-action{margin-top:1em}
.call-to-action p{text-align:center}
.call-to-action p label{padding:.75em 0;display:inline-block}
.call-to-action .btn+p,.call-to-action p+.btn{margin-top:1em}
.call-to-action .back{text-align:center;padding-top:1em;display:block}

@media all and (max-width: 599px){
	.call-to-action label{display:block;position:relative;border:1px solid #d8d8d8;border-radius:$fieldset-border-radius;text-align:left;-webkit-user-select:none;-moz-user-select:none;-ms-user-select:none;user-select:none;padding:1.125em 1em 1em 3em;margin:0 0 1em}
	.chrome .call-to-action label,.safari .call-to-action label{padding-left:4.5em;padding-bottom:1.125em}
	.call-to-action label input[type='checkbox']{position:absolute;left:1em;top:1em}
}

@media all and (min-width: 600px){
	table+.call-to-action,.section__blank-slate+.call-to-action{margin-top:1.5em}
	.call-to-action .btn{float:right}
	.call-to-action p{padding:1em 0;text-align:left;float:left}
	.call-to-action p label{padding:0}
	.call-to-action .btn+p,.call-to-action p+.btn{margin-top:0}
	.call-to-action .back{padding:1em 0;text-align:left;float:left}
	.call-to-action label{float:left;margin:$field-padding 0 0;-webkit-user-select:none;-moz-user-select:none;-ms-user-select:none;user-select:none}
	.call-to-action--final label{margin:1.5em 0 0}
}

