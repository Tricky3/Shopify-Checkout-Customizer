.chrome input[type="radio"]:checked:hover,.safari input[type="radio"]:checked:hover{border-color:#2a9dcc}
@media all and (min-width: 600px){
	.radio-group input[type="radio"]{
		top:1.125em;
		left:$field-padding
	}
}


/*==================================== Fieldsets ==================================*/
.fieldset{
	border:1px solid $fieldset-border-color;
	border-radius:$fieldset-border-radius;
	background:$fieldset-bg-color;
	margin-bottom:1.5em
}
.fieldset:last-child{margin-bottom:0}
.field{
	position:relative;
	border-top:1px solid #dfdfdf;
	border-radius:$fieldset-border-radius;
	padding-left:1em;
	padding-right:1em;
}
@media all and (min-width: 600px){
	.field{padding-left:$field-padding}
}
@media all and (min-width: 600px){
	.field{padding-right:$field-padding}
}

.field__icon{position:absolute;top:50%;right:1em}
@media all and (min-width: 600px){
	.field__icon{right:$field-padding}
}

.field--has-inset-btn input[type="text"],.field--has-inset-btn input[type="tel"],.field--has-inset-btn input[type="email"],.field--has-inset-btn input[type="password"],.field--has-inset-btn select{
	width:22%;
	padding-right:4.5em
}
.boxsizing .field--has-inset-btn input[type="text"],.boxsizing .field--has-inset-btn input[type="tel"],.boxsizing .field--has-inset-btn input[type="email"],.boxsizing .field--has-inset-btn input[type="password"],.boxsizing .field--has-inset-btn select{
	-webkit-box-sizing:border-box;
	-moz-box-sizing:border-box;
	box-sizing:border-box;
	width:65%
}
.field--has-inset-btn .field__error-message{padding-right:5em}
.field--has-radio-button{padding-left:3.125em}
@media all and (min-width: 600px){
	.field--has-radio-button{padding-left:3.375em}
}
.field--has-radio-button input[type="radio"]{position:absolute;top:1em;left:1em}
@media all and (min-width: 600px){
	.field--has-radio-button input[type="radio"]{top:$field-padding}
}
@media all and (min-width: 600px){
	.field--has-radio-button input[type="radio"]{left:$field-padding}
}

.field__inner--horizontal-padding,.field label,.field input[type="text"],.field input[type="tel"],.field input[type="email"],.field input[type="password"],.field select{
	padding-top:$field-padding;
	padding-bottom:$field-padding;
}
@media all and (min-width: 600px){
	.field__inner--horizontal-padding,.field label,.field input[type="text"],.field input[type="tel"],.field input[type="email"],.field input[type="password"],.field select{
		padding-top:$field-padding;
	}
}
@media all and (min-width: 600px){
	.field__inner--horizontal-padding,.field label,.field input[type="text"],.field input[type="tel"],.field input[type="email"],.field input[type="password"],.field select{
		padding-bottom:$field-padding;
	}
}
.field label{width:35%;float:left}
.field label .icon{margin-left:.5em}
.field .label--full-width{width:100%}

input[type="text"],input[type="tel"],input[type="email"],input[type="password"],select,.select-group{
	width:65%;
	float:left
}
.select-group{position:relative}
.select-group:after{
	content:"";display:block;
	background:#e5e5e5;
	position:absolute;top:50%;
	left:50%;
	width:1px;
	height:24px;
	margin-top:-12px;
	margin-left:-0.5px
}
.select-group select{width:44%;float:right}
.select-group select:first-child{float:left}
.field-group--has-subfields{background-color:#f9f9f9}
.field--blank-slate{text-align:center;padding:2em}
@media all and (min-width: 600px){
	.field--blank-slate{padding:2.5em}
}

.field--blank-slate .icon{display:block;margin:0 auto 1.5em}
.field--error{background:#fff7f7;z-index:2}
.boxshadow .field--error{border:none;-webkit-box-shadow:0 0 0 1px #ffc4c4;box-shadow:0 0 0 1px #ffc4c4}
.field--error ::-webkit-input-placeholder{color:#f4dede}
.field--error :-moz-placeholder{color:#f4dede;opacity:1}
.field--error ::-moz-placeholder{color:#f4dede;opacity:1}
.field--error :-ms-input-placeholder{color:#f4dede}
.field--error label,.field--error input[type="text"],.field--error input[type="tel"],.field--error input[type="email"],.field--error input[type="password"],.field--error select,.field--error .applied-gift-card{padding-bottom:0.5em}
@media all and (min-width: 600px){
	.field--error label,.field--error input[type="text"],.field--error input[type="tel"],.field--error input[type="email"],.field--error input[type="password"],.field--error select,.field--error .applied-gift-card{padding-bottom:0.625em}
}

.field--error .field__error-message{display:block}
.field__error-message{display:none;color:#ff6d6d;clear:both;padding-left:35%;margin-bottom:0;font-size:11px;font-size:0.875rem;padding-bottom:1em}
@media all and (min-width: 600px){
	.field__error-message{padding-bottom:$field-padding}
}

.boxshadow .field--focus{
	z-index:99;
	-webkit-box-shadow:0 0 0 2px $field_focus-color;
			box-shadow:0 0 0 2px $field_focus-color;
	border-color:transparent;
	border-radius:$fieldset-border-radius !important
		}
@media all and (min-width: 600px){
	.field--error:after{content:">";display:inline-block;font-family:'Checkout Symbols';font-size:$field-padding;color:#ff6d6d;position:absolute;top:10px;left:-2em;padding:0 .25em;-webkit-animation:slideInLeft .8s ease-out both, back-and-forth .5s ease-in infinite both alternate;animation:slideInLeft .8s ease-out both, back-and-forth .5s ease-in infinite both alternate}
}


/* ==================================== Tables ====================================*/
table{border-collapse:collapse;border-spacing:0}
table{width:100%;border-collapse:collapse;border-spacing:0;border-bottom:1px solid #ededed}
td,th{padding:0}.js [data-hidden-if-js]{display:none}.no-js [data-shown-if-js]{display:none}
th{color:#555;text-align:center;font-weight:500;padding-bottom:1em;border-bottom:1px solid #ededed}
th:first-child{text-align:left}
th:last-child{text-align:right}
tr:first-child td{border-top:none}
tr td{border-top:1px solid #ededed}
td{color:#999;text-align:center;padding:1.5em 0}
td:first-child{text-align:left}td:last-child{text-align:right}
td small{color:#ccc;display:block;font-size:11px;font-size:0.875rem}
td strong{font-weight:500;color:#777}

@media all and (min-width: 600px){
	td{padding:2em 0}
}

/*=============================== Inventory Issues ================================*/
.inventory-issues-table .cell--quantity{text-align:right}
.inventory-issues-table .cell--quantity .quantity{display:inline-block;margin-bottom:.2em}
.inventory-issues-table .product__info{margin:.4em 0}
.status-label{padding-left:20px;background-position:center left !important}
.status-label--soldout{color:#ee7272;background-image:url(//cdn.shopify.com/s/assets/checkout/icons/status-sold-out.svg-50a91adf0f7b1558ed4c01b30acbacf8.png);background-image:url(//cdn.shopify.com/s/assets/checkout/icons/status-sold-out-50a91adf0f7b1558ed4c01b30acbacf8.svg),none;background-position:center center;background-repeat:no-repeat}
.status-label--reduced{color:#e7a321;background-image:url(//cdn.shopify.com/s/assets/checkout/icons/status-reduced.svg-03c1006c9e82dcc718d5761f3a4ea638.png);background-image:url(//cdn.shopify.com/s/assets/checkout/icons/status-reduced-03c1006c9e82dcc718d5761f3a4ea638.svg),none;background-position:center center;background-repeat:no-repeat}

@media all and (min-width: 600px){
	.inventory-issues-table .cell--status{position:relative}
	.inventory-issues-table .cell--status .btn--delete{float:none;position:absolute;right:-45px;top:50%;margin-top:-8px}
	.inventory-issues-table .cell--quantity{text-align:center}
	.inventory-issues-table .cell--status{text-align:right}
}

@media all and (max-width: 599px){
	.inventory-issues-table .cell--status{display:none}
}

@media screen and (max-width: 999px){
	.inventory-issues-table .cell--price{display:none}
}

/*=============================== Thank you Section ===============================*/
.section--thank-you{text-align:center}
.section--thank-you p strong{display:block;margin-top:.5em}
.section--thank-you .svg+h3{margin:1em auto .5em}
.section--thank-you .btn+p{margin-top:1.5em}


/*=============================== CHECKOUT STRUCTURE ===============================
									[1.0] : Header
									[2.0] : Main
									[3.0] : Footer
===================================================================================*/

/*===============================Ever-present wraps================================*/
.wrap{margin:0 auto;width:100%;zoom:1}
.wrap:after,.wrap:before{content:"";display:table}
.wrap:after{clear:both}
.no-generatedcontent .wrap{overflow:hidden}

@media all and (min-width: 600px){
	.wrap{width:73%;padding:0 12.5%}
}

@media screen and (min-width: 800px){
	.wrap{width:60%}
}

@media screen and (min-width: 1000px){
	.wrap{padding:0 5em;width:auto;max-width:76em}
}

@media screen and (min-width: 1100px){
	.wrap{padding:0 5em}
}

/*=========================== Header and shop ID [1.1] ============================*/
.header{padding:$header-padding;position:relative;zoom:1;overflow:hidden;background:$header-background-color;}
.header .icon--lock{
	position:absolute;
	top:1em;right:1em;
	z-index:2;
	background-image:url(//cdn.shopify.com/s/assets/checkout/icons/ico-lock-grey.svg-b4b8945a4e3cfa0cab824cc584cd2627.png);
	background-image:url(//cdn.shopify.com/s/assets/checkout/icons/ico-lock-grey-b4b8945a4e3cfa0cab824cc584cd2627.svg),none;
	background-position:center center;background-repeat:no-repeat
}

.shop{z-index:1;position:relative;float:left}
.shop__name{z-index:2;position:relative;color:$shop-name-color;font-weight:400;-webkit-transition:none;transition:none;}
.shop__desc{z-index:2;position:relative;color:#fff;line-height:$line-height;color:#606060}
.rgba .shop__desc{color:rgba(51,51,51,0.7)}
.shop__logo{display:block;max-width:50%}
.shop__logo+.shop__desc{margin-top:.35em}

@media all and (min-width: 600px){
	.header{padding:$header-padding;}
	.shop__logo{max-width:200px}
	.shop__desc{font-weight:300;font-size:17px;font-size:1.2rem}
}

@media screen and (min-width: 1000px){
	.header{padding:$header-padding;}
	.shop__desc{font-size:20px;font-size:1.4rem}
}



/*================================== Main [2.1] ===================================*/
.main{height:100%;width:100%}

