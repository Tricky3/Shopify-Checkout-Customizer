@charset "UTF-8";
@import url("https://fast.fonts.com/cssapi/4b266a0c-acd1-4016-8e36-45aaf6a347fa.css");

/* Take the Risk to Reinvent */

/*==============================GLOBAL VARS ==============================================/*
/* font-family */						$font-stack:'BasicCommercialW01-Roma', Futura, "Trebuchet MS", Arial, sans-serif;
/* base font-size */					$base-font-size:14px;
/* Primary Color, used for body text */ $primary-color:#6d6f71;
/* line height */						$line-height:1.5em;
/* href color, normal status */			$href-normal-color:#4c99c2;
/* href color, hover status */			$href-hover-color:#0e1f2e;
/* selection background color */		$selection-bg-color:#0e1f2e;
/* selection color */					$selection-color:#ffffff;
/* heading color */						$headings-color:#333;
/* h1 font-size */						$h1-font-size:24px;
/* h3 font-size */						$h3-font-size:16px;
/* h1 font-size */						$h5-font-size:12px;
/*==============================FORM VARS ==============================================/*
/* label color */						$label-color:#777;
/* fieldset border color */				$fieldset-border-color:#d8d8d8;
/* fieldset border radius  */			$fieldset-border-radius:0; 
/* used for other elems too */
/* fieldset background color */			$fieldset-bg-color:#fff;
/* field focus color */					$field_focus-color:#6d6f71;
/* field padding */						$field-padding:1em;
/*==============================LAYOUT VARS ==============================================/*
/* header background color*/			$header-background-color:#0f202f;
/* header padding */					$header-padding:12px 0;
/* shop name color */					$shop-name-color:#fff;
/* order summary background-color */	$order-summary-background-color:#fff;
/* order summary border-color */		$order-summary-border-color:#dadada;
/* order summary color */				$order-summary-color:#6d6f71;
/* footer background-color */			$footer-bg-color:#fff;
/* footer-padding*/						$footer-padding:1.5em
/* footer-top-border-color*/			$footer-top-border-color:#e5e5e5;
/* footer-href-color*/					$footer-href-color:#999;
/* footer-href-hover-color*/			$footer-href-hover-color:#2486ae;
/* footer-text-color*/					$footer-text-color:#999;
/* footer-font-size*/					$footer-font-size:11px;




/*==============================RESPONSIVE LAYOUT VARS ==============================================*/
$media-all-for-mobile: "all and (max-width: 599px)";
$media-all-for-tablet: "all and (min-width: 600px)";
$media-all-for-desktop: "all and (min-width: 1000px)";
$media-all-for-large-desktop: "all and (min-width: 1100px)";


body, button, input, select, textarea {
  font-family: $font-stack;
  color:$primary-color;
}

/*========================== the temple of the soul ===============================*/
html{font-family:$font-stack;font-size:$base-font-size;}
@media all and (min-width: 600px){
	html{font-size:$base-font-size;}
}
body{margin:0}

/*================================= HREFS =========================================*/
a:active,
a:hover{outline:0}
a{
	color:$href-normal-color;
	text-decoration:none;
}
a:hover,a:focus{color:$href-hover-color;outline:0}

/*===========================general Mambo jambo===================================*/
.money{color:#8fc243}


/*================================ selections =====================================*/
::-moz-selection{background:$selection-bg-color;color:$selection-color;}
::selection{background:$selection-bg-color;color:$selection-color;}

/*===================================Buttons ======================================*/
button,input,optgroup,select,textarea{color:$primary-color;font:inherit;margin:0}
button,select{text-transform:none}

/* ================================== Headings ====================================*/
h1,h2,h3,h4,h5,h6{margin:0;font-weight:400;text-transform:normal;letter-spacing:normal;color:$headings-color;}
h1,h2{font-size:$h1-font-size;}
h3,h4{font-size:$h3-font-size}
h5,h6{font-size:$h5-font-size}

/*=================================== paragraphs ==================================*/
p{
	color:$primary-color;
	margin:0;
	line-height:$line-height;
	margin-bottom:1.5em
}
p:last-child{margin-bottom:0}
strong{color:$primary-color;}


/*================================= FORM related ==================================*/
form{margin:0}
input{line-height:normal}
input[type="checkbox"],input[type="radio"]{padding:0}
fieldset{
	border:1px solid #c0c0c0;
	margin:0 2px;
	padding:0.35em 0.625em 0.75em
}
legend{border:0;padding:0}
optgroup{font-weight:bold}

label{color:$label-color;line-height:$line-height;}
label small{font-size:1em;color:$label-color;}
input,select{color:$primary-color;}
input[type="text"],input[type="tel"],input[type="email"],input[type="password"],select{line-height:$line-height;}
input[type="text"]:-webkit-autofill,input[type="tel"]:-webkit-autofill,input[type="email"]:-webkit-autofill,input[type="password"]:-webkit-autofill,select:-webkit-autofill{
	-webkit-text-fill-color:$primary-color;
	-webkit-box-shadow:0 0 0 50px #fff inset;
	box-shadow:0 0 0 50px #fff inset
}
::-webkit-input-placeholder{color:$label-color}
:-moz-placeholder{color:$label-color;}
::-moz-placeholder{color:$label-color;}
:-ms-input-placeholder{color:$label-color}

.boxsizing select{padding-right:2em}
.chrome select,.safari select{
	-webkit-appearance:none;
	background-image:url(//cdn.shopify.com/s/assets/checkout/icons/ico-updown.svg-4c5ac8f8d447206ab65a5d392d3dcd97.png);
	background-image:url(//cdn.shopify.com/s/assets/checkout/icons/ico-updown-4c5ac8f8d447206ab65a5d392d3dcd97.svg),none;background-position:center center;
	background-repeat:no-repeat;
	background-position:right center
}
.chrome select:focus,.safari select:focus{
	background-image:url(//cdn.shopify.com/s/assets/checkout/icons/ico-updown-focus.svg-ca782fe792487c2bbdaa07a37011714a.png);
	background-image:url(//cdn.shopify.com/s/assets/checkout/icons/ico-updown-focus-ca782fe792487c2bbdaa07a37011714a.svg),none;
	background-position:center center;
	background-repeat:no-repeat;
	background-position:right center
}
input[type='checkbox']{margin-right:.25em;-webkit-appearance:none;cursor:pointer;position:relative}

